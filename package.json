{
  "name": "vocab-tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prepare:packs": "node ./scripts/prepare-packs.js",
    "cards:draft": "node ./scripts/corpus-to-cards.js --level A2",
    "cards:smoke": "node ./scripts/corpus-to-cards.js --level A2 --tokens ./.codex-local/corpus/simplewiki/data/tokens.csv.gz --out ./cards/draft_cards.jsonl",
    "cards:filter": "node ./scripts/corpus-to-cards.js --sfwLevel default --dropProperNouns on",
    "cards:filter:strict": "node ./scripts/corpus-to-cards.js --sfwLevel strict --dropProperNouns on --level A2",
    "packs:from-cards": "node ./scripts/cards-to-packs.js --cards cards/draft_cards.jsonl --level A2 --outDir public/packs/A2",
    "packs:from-cards:matching": "node ./scripts/cards-to-packs.js --cards cards/draft_cards.jsonl --level A2 --outDir public/packs/A2",
    "packs:from-cards:safe": "node ./scripts/cards-to-packs.js --sfwLevel default --dropProperNouns on --cards cards/draft_cards.jsonl --level A2 --outDir public/packs/A2",
    "packs:from-cards:strict": "node ./scripts/cards-to-packs.js --sfwLevel strict --dropProperNouns on --cards cards/draft_cards.jsonl --level A2 --outDir public/packs/A2",
    "packs:validate": "node ./scripts/packs-validate.js --dir public/packs/A2 --type auto",
    "packs:validate:strict": "node ./scripts/packs-validate.js --dir public/packs/A2 --type auto --sfwLevel strict --strict",
    "packs:convert:matching": "node ./scripts/convert-matching-set-to-pairs.js --in public/packs/A2/matching.csv --out public/packs/A2/matching.pairs.csv"
  },
  "dependencies": {
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
