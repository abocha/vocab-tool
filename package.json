{
  "name": "vocab-tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "test": "vitest --run --pool=threads",
    "preview": "vite preview",
    "prepare:packs": "node ./scripts/prepare-packs.js",
    "cards:draft": "node ./scripts/corpus-to-cards.js",
    "packs:from-cards": "node ./scripts/cards-to-packs.js",
    "packs:validate": "node ./scripts/packs-validate.js --type auto",
    "packs:convert:matching": "node ./scripts/convert-matching-set-to-pairs.js --in public/packs/A2/matching.csv --out public/packs/A2/matching.pairs.csv"
  },
  "dependencies": {
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vitest": "^3.2.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
